{
    "name": "alexbuckham/cloudflare-images-laravel",
    "description": "Provides access to the Cloudflare Images service for Laravel.",
    "keywords": [
      "alexbuckha",
      "cloudflare",
      "images",
      "php",
      "laravel"
    ],
    "authors": [
        {
            "name": "Alex Buckham",
            "email": "alexbuckham@me.com"
        }
    ],
    "minimum-stability": "dev",
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/contracts": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "AlexBuckham\\CloudflareImagesLaravel\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "AlexBuckham\\CloudflareImagesLaravel\\CloudflareImagesProvider"
            ]
        }
    }
}
